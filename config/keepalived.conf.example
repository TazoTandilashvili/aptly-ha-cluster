# Keepalived Configuration Example

# This is an example configuration file for Keepalived.

global_defs {
   router_id LVS_DEVEL
}

vrrp_instance VI_1 {
    state MASTER
    interface ens160
    virtual_router_id 51
    priority 101
    advert_int 1
    authentication {
        auth_type PASS
        auth_pass your_password
    }
    virtual_ipaddress {
        10.80.11.140
    }
}

track_script {
    script "killall -0 haproxy"
    interval 2
    weight 2
}
